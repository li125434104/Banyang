#import "UIImageView+OCAdditions.h"

@implementation UIImageView (OCAdditions)

+ (UIImageView *)imageViewWithImageNamed:(NSString *)name {
    return [[UIImageView alloc] initWithImage:[UIImage imageNamed:name]];
}

@end
